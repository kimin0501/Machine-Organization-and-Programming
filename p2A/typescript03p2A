[33m 64 [m   size_chars = strtok(line1, DELIM);
[33m 65 [m   *size = atoi(size_chars);[37;89H[K[37;89H65,1[10C23%[36;5H[?25h[?25l[1;36r[1;1H[4M[1;37r[33;1H[33m 66 
