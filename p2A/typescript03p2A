[33m 52 [m[36m */[m
[33m 53 [m[32mvoid[m get_board_size([32mFILE[m *fptr, [32mint[m *size) {
[33m 54 [m   [32mchar[m *line1 = [38;5;130mNULL[m;
[33m 55 [m   [32msize_t[m len = [38;5;130m0[m;
[33m 56 
